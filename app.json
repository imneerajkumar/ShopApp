{
  "expo": {
    "name": "ShopApp",
    "slug": "ShopApp",
    "description": "This is a clone of a online shopping app, implementing features like auth, adding products to cart, ordering as well as adding their own products.",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/tag.png",
    "splash": {
      "image": "./assets/tag.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.imneerajKumar.shop-app",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.imneerajKumar.shop_app",
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/tag.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"]
    },
    "notification": {
      "icon": "./assets/tag.png",
      "iosDisplayInForeground": true
    }
  }
}
